{
  "name": "strencoderkit",
  "type": "module",
  "version": "0.0.0",
  "description": "產生類似於熊曰、魔曰之類的編碼工具，但編碼文字可以自訂，目前做出來是為了好玩，並無考慮實際用途，不要用到生產環境中。",
  "license": "GPL-3.0",
  "homepage": "https://github.com/ycs77/strencoderkit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycs77/strencoderkit.git"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/strencoderkit.d.ts",
      "import": "./dist/strencoderkit.js"
    }
  },
  "main": "./dist/strencoderkit.js",
  "types": "./dist/strencoderkit.d.ts",
  "bin": {
    "strencoderkit": "bin/strencoderkit.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:pkg": "tsup",
    "build:cli": "tsup --config tsup.cli.config.ts",
    "watch": "run-p watch:*",
    "watch:pkg": "tsup --watch",
    "watch:cli": "tsup --watch --config tsup.cli.config.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@clack/prompts": "^0.10.0",
    "cac": "^6.7.14",
    "js-crypto-aes": "^1.0.6",
    "js-crypto-hash": "^1.0.7",
    "pako": "^2.1.0",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/pako": "^2.0.3",
    "npm-run-all2": "^7.0.2",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "~5.7.3",
    "unishox2.siara.cc": "^1.1.5",
    "vitest": "^3.0.9"
  }
}
