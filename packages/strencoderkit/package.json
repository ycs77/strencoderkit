{
  "name": "strencoderkit",
  "type": "module",
  "version": "0.6.1",
  "description": "可自訂編碼文字的編碼工具，目前做出來只是為了好玩和研究學習，加密內容不保證安全，不建議用到實際情境中。",
  "author": {
    "name": "Lucas Yang",
    "email": "yangchenshin77@gmail.com"
  },
  "license": "AGPL-3.0",
  "homepage": "https://github.com/ycs77/strencoderkit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycs77/strencoderkit.git",
    "directory": "packages/strencoderkit"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/strencoderkit.js",
      "require": "./dist/strencoderkit.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/strencoderkit.cjs",
  "module": "./dist/strencoderkit.js",
  "types": "./dist/strencoderkit.d.cts",
  "bin": {
    "strencoderkit": "bin/strencoderkit.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p build:\\*",
    "build:pkg": "tsdown",
    "build:cli": "tsdown --config tsdown.cli.config.ts",
    "watch": "run-p watch:\\*",
    "watch:pkg": "tsdown --watch",
    "watch:cli": "tsdown --watch --config tsdown.cli.config.ts",
    "test": "vitest",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "@clack/prompts": "catalog:prod",
    "cac": "catalog:prod",
    "js-crypto-aes": "catalog:prod",
    "js-crypto-hash": "catalog:prod",
    "pako": "catalog:prod",
    "picocolors": "catalog:prod"
  },
  "devDependencies": {
    "@types/node": "catalog:dev",
    "@types/pako": "catalog:dev",
    "npm-run-all2": "catalog:dev",
    "tsdown": "catalog:dev",
    "tsx": "catalog:dev",
    "typescript": "catalog:dev",
    "unishox2.siara.cc": "catalog:dev",
    "vitest": "catalog:dev"
  }
}
